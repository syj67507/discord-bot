{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean": "tsc --build --clean && npm run clean-media && npm run clean-build && npm run clean-gcp && npm run clean-test",
        "clean-media": "rimraf -f media/say.mp3",
        "clean-build": "rimraf -rf dist",
        "clean-gcp": "rimraf -f googleApplicationCredentials.json",
        "clean-test": "rimraf -rf coverage",
        "build": "tsc",
        "prestart": "npm i ytdl-core@latest ytsr@latest",
        "start": "node dist/v12_src/index.js",
        "test": "jest --passWithNoTests --coverage",
        "dev": "nodemon --ext ts --exec \"npm run clean && npm run build && node dist/v12_src/index.js\""
    },
    "author": "",
    "license": "ISC",
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ],
            "@babel/preset-typescript"
        ]
    },
    "jest": {
        "testEnvironment": "node"
    },
    "dependencies": {
        "@discordjs/opus": "^0.3.2",
        "@google-cloud/text-to-speech": "^3.2.0",
        "discord.js": "^12.2.0",
        "discord.js-commando": "github:discordjs/Commando",
        "dotenv": "^8.2.0",
        "ffmpeg-static": "^4.2.7",
        "giphy-js-sdk-core": "^1.0.6",
        "pokedex-promise-v2": "^3.2.0",
        "winston": "^3.3.3",
        "ytdl-core": "^4.9.1",
        "ytsr": "^3.5.2"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@types/jest": "^27.0.1",
        "@types/node": "^14.14.20",
        "babel-jest": "^27.0.6",
        "eslint": "^7.8.1",
        "jest": "^26.5.2",
        "nodemon": "^2.0.7",
        "typescript": "^4.1.3"
    }
}
